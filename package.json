    "url": "https://github.com/loerk/loerk.github.io/issues"
  },
  "dependencies": {
    "tsparticles": "^2.12.0",
    "tsparticles-engine": "^2.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "prettier": "^3.5.3",
    "sass": "^1.83.1",
    "typescript": "^5.7.3",
    "vite": "^6.2.3"
  }
